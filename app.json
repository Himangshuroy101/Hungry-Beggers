{
  "expo": {
    "name": "HungryBegger",
    "slug": "HungryBegger",
    "scheme": "hungrybeggars",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Final_logo_sq.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/Final_logo_sq.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.himangshu.hungrybegger", 
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/Final_logo_sq.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/Final_logo_sq.png"
    }
  }
}
